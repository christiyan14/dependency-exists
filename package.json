{
  "version": "1.0.0",
  "description": "Dependency Exists - Best way to check if some dependency exists in your package.json file.",
  "name": "@kv/dependency-exists",
  "author": {
    "name": "Krisityan Velkov",
    "email": "christiyanweb@gmail.com",
    "url": "https://www.linkedin.com/in/kristiyan-velkov-763130b3/"
  },
  "scripts": {
    "build-test": "yarn build:cjs && yarn build:esm",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "types": "./lib/types/index.d.ts",
  "main": "./lib/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christiyan14/dependency-exists/issues"
  },
  "homepage": "https://github.com/christiyan14/dependency-exists#readme",
  "dependencies": {
    "chalk": "^5.2.0",
    "path": "^0.12.7",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "copy-webpack-plugin": "^11.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "keywords": [
    "npm package",
    "dependencies",
    "devDependencies",
    "peerDependencies",
    "package.json",
    "ES6",
    "JavaScript",
    "TypeScript",
    "ECMAscript",
    "Node",
    "JS"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  }
}